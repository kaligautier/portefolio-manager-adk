# Application Configuration
APP_NAME=ADK_Agent_Template
APP_DESCRIPTION=Production-ready ADK agent template with best practices
APP_VERSION=0.1.0
PROJECT_NAME=adk-agent-template

# Server Configuration
HOST=0.0.0.0
PORT=8000
DEBUG=false
LOG_LEVEL=INFO

# Agent Configuration
AGENT_NAME=template_agent
MODEL=gemini-2.5-flash

# Google Cloud Platform (REQUIRED)
GOOGLE_GENAI_USE_VERTEXAI=true
GOOGLE_CLOUD_PROJECT=your-gcp-project-id
GOOGLE_CLOUD_LOCATION=us-central1

# Session Management
USER_ID=api_user

# Vertex AI RAG Corpus (REQUIRED if using RAG)
RAG_CORPUS_ID=projects/PROJECT_ID/locations/LOCATION/ragCorpora/CORPUS_ID

# Vertex AI Search Datastore (REQUIRED if using Search)
SEARCH_DATASTORE_ID=projects/PROJECT_ID/locations/LOCATION/collections/default_collection/dataStores/DATASTORE_ID

# MCP Toolbox - PostgreSQL (Optional)
TOOLBOX_PG_URL=http://127.0.0.1:5000
TOOLBOX_PG_TOOLSET=postgres_database_tools

# MCP Toolbox - BigQuery (Optional)
BIGQUERY_PROJECT=your-gcp-project-id
BIGQUERY_LOCATION=US
BIGQUERY_USE_CLIENT_OAUTH=false
TOOLBOX_BQ_URL=http://127.0.0.1:5001
TOOLBOX_BQ_TOOLSET=bigquery_database_tools

# Vertex AI Agent Engine Sessions (Optional - for managed sessions)
AGENT_ENGINE_ID=
USE_AGENT_ENGINE_SESSIONS=false
